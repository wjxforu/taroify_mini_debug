{
  "extends": "stylelint-config-standard",
  "rules": {
    "property-no-unknown": [
      true,
      {
        "ignoreProperties": [
          "composes",
          "compose-with"
        ]
      }
    ],
    "color-hex-case": "upper",
    "selector-type-no-unknown": [true, {
      "ignoreTypes": [
        "/^page/"
      ]
    }]
  }
}
